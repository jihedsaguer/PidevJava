<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="58.0" prefWidth="725.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.esprit.controllers.CoachComponent">
   <children>
      <HBox prefHeight="58.0" prefWidth="734.0">
         <children>
            <Label fx:id="TFname" prefHeight="57.0" prefWidth="186.0" text="Name" />
            <Label fx:id="TFemail" prefHeight="60.0" prefWidth="178.0" text="Date" />
            <ImageView fx:id="img" fitHeight="57.0" fitWidth="96.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets right="120.0" />
               </HBox.margin>
               <image>
                  <Image url="@img/defaultusericon.jpg" />
               </image>
            </ImageView>
            <Button mnemonicParsing="false" onAction="#supprimerButton" prefHeight="58.0" prefWidth="76.0" text="supprimer" />
            <Button mnemonicParsing="false" onAction="#modifierButton" prefHeight="58.0" prefWidth="82.0" text="modifier" />
         </children>
      </HBox>
   </children>
</AnchorPane>
