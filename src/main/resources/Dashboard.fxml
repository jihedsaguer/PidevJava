<AnchorPane prefHeight="498.0" prefWidth="863.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controllers.Dashboard">
   <children>
      <Label layoutX="20.0" layoutY="20.0" prefHeight="40.0" prefWidth="823.0" text="Users List">
