      <TextField fx:id="searchField" layoutX="372.0" layoutY="20.0" prefHeight="21.0" promptText="chercher .." />
      <Pagination fx:id="pagination" layoutX="20.0" layoutY="446.0" />

      <Button layoutX="691.0" layoutY="20.0" mnemonicParsing="false" onAction="#search" prefWidth="74.0" text="search" />

   </children>
